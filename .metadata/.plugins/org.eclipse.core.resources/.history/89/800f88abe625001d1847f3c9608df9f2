package CodingPrograms;

public class prog9 {
  public static void main(String[] args) {
	
	  
	  //Write program to find Prime number from 1 to 100
	  
	   int val=31;
	   boolean  fag= false;
	   
	   
	   for( int i= 2;i<=31/2;i++)
	   
	   
	   {
		    if(val%i==0) 
		    {
		    	fag= true;
		    	break;
		    }
		    
		    if(fag) 
		    	System.out.println("it is not prime");
		    else 
				
		    	System.out.println("it is  prime");
		   
	   }
	  
	  
	  
	  
}
}
