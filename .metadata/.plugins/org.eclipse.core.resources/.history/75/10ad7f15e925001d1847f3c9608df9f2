package CodingPrograms;

public class Prog10 {

	
	public static void main(String[] args) {
		
		/*2,2,5
		  3,5,7
		 {1,9,8
		 */
	
		
		 int abc[][] ={{2,2,5},{3,5,7},{1,9,8}};
		 int min=abc[0][0];
		 
		  for(int i=0;i<3;i++) {
			  for( int j=0;j<3;j++) {
				  if(abc[i][j]<min) {
					  min =abc[i][j];
				  }
			  }
		  }System.out.println(abc);
		 
	}
	
}
