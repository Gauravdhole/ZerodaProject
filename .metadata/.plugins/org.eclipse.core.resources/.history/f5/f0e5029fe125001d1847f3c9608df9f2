package CodingPrograms;

public class Prog7 {
  public static void main(String[] args) {
	//programe for prints pyridymaeds
	  
	  
	  
	  System.out.println("*");
	  for(int i=1;i<5;i++) {
		  for( int j=1;j<i;j++) {
			 System.out.println("*"); 
			 System.out.println("\t");
		  }
		  System.out.println();
	  }
	  
	  
	  
	  
}
}
